<script> 
$(document).ready(function(){
   
    $("#op-uno").click(function(){
        $("#uno").fadeIn('500');
	$("#dos,#tres,#cuatro,#cinco").fadeOut('500');
    });
 
    $("#op-dos").click(function(){
        $("#dos").fadeIn('500');
	$("#uno,#tres,#cuatro,#cinco").fadeOut('500');
    });
    
    $("#op-tres").click(function(){
        $("#tres").fadeIn('500');
	$("#dos,#uno,#cuatro,#cinco").fadeOut('500');
    });
   
    $("#op-cuatro").click(function(){
        $("#cuatro").fadeIn('500');
	$("#dos,#tres,#uno,#cinco").fadeOut('500');
    });
    
    $("#op-cinco").click(function(){
        $("#cinco").fadeIn('500');
	$("#dos,#tres,#uno,#cuatro").fadeOut('500');
    });
    
});

$(document).ready(function() {
  // audio player
  var $audio = $('iframe.tumblr_audio_player');
  $audio.load(function() {
    $(this).contents().find('head').append('<style type="text/css">' +
      '.audio-player { background:{color:AudioPlayerBg}; color: {color:AudioPlayerText} !important; }' +
      '.audio-player .audio-info .track-artist { color: {color:AudioPlayerText} !important; }' +
    '</style>');
  });
});

(function($){$(document).ready(function(){$("a[title]").style_my_tooltips({tip_follows_cursor:true,tip_delay_time:90,tip_fade_speed:100,attribute:"title"});});})(jQuery);

</script>

 
